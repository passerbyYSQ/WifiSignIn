<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.frag.group.GroupSearchFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycler_group_search"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_main_recycler"
        android:paddingTop="@dimen/len_12"
        android:paddingBottom="@dimen/len_12"
        android:clipToPadding="false"
        android:elevation="1dp"
        android:outlineProvider="bounds"
        tools:listitem="@layout/item_joined_group"
        tools:targetApi="lollipop" />

</FrameLayout>